properties([[$class: 'GithubProjectProperty', displayName: '', projectUrlStr: 'https://github.com/dev-demo-project/developers/'], gitLabConnection(''), [$class: 'GitlabLogoProperty', repositoryName: ''], pipelineTriggers([githubPush()])])

